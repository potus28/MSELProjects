  </HarmonicAngleForce>
  <RBTorsionForce>
    <!--OPLS style torsions have been converted to Ryckaert-Bellemans style.-->
  </RBTorsionForce>
  <NonbondedForce coulomb14scale="0.0" lj14scale="0.0">
      <Atom type="TrSM_O_1" charge="-0.35" sigma="0.305" epsilon="0.6568423290544378"/>
    <Atom type="TrSM_C_1" charge="0.7" sigma="0.280" epsilon="0.22449041625911167"/>
  </NonbondedForce>
</ForceField>
